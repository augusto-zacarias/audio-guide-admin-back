{
 "compilerOptions": {
   "target": "esnext",
   "module": "ESNext",
   "strict": true,
   "esModuleInterop": true,
   "skipLibCheck": true,
   "outDir": "./dist",
   "resolveJsonModule": true,
   "allowSyntheticDefaultImports": true,
   "moduleResolution": "Bundler",
   
 },
 "include": [
   "./**/*.ts"
 ],
 "exclude": [
   "dist",
   "node_modules"
 ]
}

